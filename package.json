  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wilx/stupid-table-plugin-cash-dom/issues"
  },
  "homepage": "https://github.com/wilx/stupid-table-plugin-cash-dom",
  "packageManager": "yarn@3.0.2"
}
