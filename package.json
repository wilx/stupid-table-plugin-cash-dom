{
  "name": "stupid-table-plugin-cash-dom",
  "version": "1.1.25",
  "description": "Stupidly simple cash-dom table sorting plugin",
  "main": "./dist/stupidtable.js",
  "scripts": {
    "build": "yarn version -i patch && yarn eslint src/** && yarn webpack",
